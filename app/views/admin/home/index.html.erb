<div class="row mb-4"></div>
<div class="row">
  <div class="col-2  border border-black d-inline-flex p-2" style="text-align: center; align-items: center; justify-content: center;">
    System Management
  </div>
  <div class="col-7  border border-black"></div>
  <div class="col-1  border border-black d-inline-flex p-2" style="text-align: center; align-items: center; justify-content: center;">
    Change Language
  </div>
  <div class="col-2  border border-black d-inline-flex p-2" style="text-align: center; align-items: center; justify-content: center;">
    <li class="list-group-item" style="border: 1px solid black; border-radius: 0%; ">
      <div class="dropdown">
        <button class="btn btn-secondary dropdown-toggle w-auto" type="button" id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false">
          Username
        </button>
        <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton1">
          <li><%= link_to 'Profile', admin_root_path, class: "nav-link #{'active' if action_name == 'posts' }", 'aria-current': 'page' %>  </li>
          <li><%= link_to 'Sign Out', destroy_admin_user_session_path, data: { 'turbo-method': :delete }, class: "nav-link #{'active' if action_name == 'sign in' }", 'aria-current': 'page' %></li>
        </ul>
      </div>
    </li>
  </div>
</div>

<div class="row">
  <div class="col-2  border border-black border-top-o">

    <div class="accordion accordion-flush" id="accordionFlushExample">
      <div class="accordion-item" style="margin-left:20px;">
        Home Page
      </div>
      <div class="accordion-item">
        <h2 class="accordion-header" id="flush-headingTwo">
          <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseTwo" aria-expanded="false" aria-controls="flush-collapseTwo">
            Member Management
          </button>
        </h2>
        <div id="flush-collapseTwo" class="accordion-collapse collapse" aria-labelledby="flush-headingTwo" data-bs-parent="#accordionFlushExample">
          <div class="accordion-body">
            <li>User List</li>
            <li>Item List</li>
          </div>
        </div>
      </div>

      <div class="accordion-item">
        <h2 class="accordion-header" id="flush-headingThree">
          <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseThree" aria-expanded="false" aria-controls="flush-collapseThree">
            Example 2 Management
          </button>
        </h2>
        <div id="flush-collapseThree" class="accordion-collapse collapse" aria-labelledby="flush-headingThree" data-bs-parent="#accordionFlushExample">
          <div class="accordion-body">
            <li>Example 1</li>
            <li>Example 2</li>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-10">
    <div class="row">
      <h5>Hi, Admin: <%= @admin_user.email %></h5>
    </div>
    <div class="row">
      <% @client_users.each do |client_user| %>
        <div class="container" style="border:1px solid black;">
          <p>Client: <%= client_user.email %>
            <% if client_user.parent&.email.present? %>
              was invited by <%= client_user.parent&.email %>
            <% end %></p>
          <p>Total Deposit = ₱ <%= client_user.total_deposit %>
          <div class="container" style="border:1px solid black;">
            <% if client_user.users.present? %>
              <p>Member's Total Deposit = ₱<%= client_user.users.sum(&:total_deposit) %></p>
              <ul>
                <% client_user.users&.each do |child| %>
                  <li><%= child.email %> = ₱<%= child.total_deposit %></li>
                <% end %>
              </ul>
            <% else %>
              <li>No Members</li>
            <% end %></div>
          </p>
          <p>Coins = ₱ <%= client_user.coins %></p>
          <p>Total Used Coins = </p>
          <div> Phone Number: <%= client_user.phone %></div>
        </div>
      <% end %>
    </div>
  </div>
</div>
